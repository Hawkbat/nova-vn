{
  "applicationId": "nova.vn",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "nativeAllowList": [
    "app.*",
    "filesystem.*"
  ],
  "globalVariables": {
    "PROJECT_DIR": "."
  },
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "modes": {
    "window": {
      "title": "Nova VN",
      "width": 960,
      "height": 540,
      "minWidth": 384,
      "minHeight": 216,
      "icon": "/favicon.png",
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "nova-vn",
    "resourcesPath": "/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/engine/neutralino.js",
    "binaryVersion": "4.12.0",
    "clientVersion": "3.10.0"
  }
}